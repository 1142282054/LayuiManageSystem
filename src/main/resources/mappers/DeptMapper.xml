<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.layuimanagesystem.emp.mapper.DeptMapper">
    <select id="getDept" resultType="com.example.layuimanagesystem.emp.entity.Dept">
        select * from dept where dept_id = #{deptId}
    </select>
    <select id="getAllDept" resultType="com.example.layuimanagesystem.emp.entity.Dept">
        select * from dept
    </select>
</mapper>
